@model Certificate

<form id="ajaxForm" class="default-form" data-ajax="true" data-ajax-method="post" data-ajax-success="OnSuccessUpSert" data-ajax-failure="OnFailureUpSert" asp-controller="Certificates" asp-action="Create" enctype="multipart/form-data">
    <div class="row">
        <div class="form-group col-lg-6 col-md-12">
            <label>Title</label>
            <input type="text" asp-for="Title" required placeholder="Title">
        </div>

        <div class="form-group col-lg-6 col-md-12">
            <label>Provider</label>
            <input type="text" asp-for="Provider" required placeholder="Provider">
        </div>

        <div class="form-group col-lg-6 col-md-12">
            <label>Certified At</label>
            <input type="date" asp-for="CertifiedAt" required placeholder="Certified At">
        </div>
    </div>
</form>
